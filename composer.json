{
    "name": "katsana/minions",
    "description": "Json-RPC Communication for Laravel",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "KATSANA Developer",
            "email": "developer@katsana.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Minions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Minions\\Tests\\": "src/"
        }
    },
    "require": {
        "php": ">=7.2",
        "datto/json-rpc": "^5.0",
        "illuminate/auth": "^5.7",
        "illuminate/console": "^5.7",
        "illuminate/http": "^5.7",
        "illuminate/pipeline": "^5.7"
    },
    "require-dev": {
        "graze/guzzle-jsonrpc": "^3.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7.5 || ^8.0",
        "react/http": "^0.8.4"
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit -c ./"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Minions\\MinionsServiceProvider"
            ],
            "aliases": {
                "Minion": "Minions\\Minion"
            }
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
